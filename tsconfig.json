{
    "compilerOptions": {
      "module": "esnext",
      "target": "es6",
      "lib": ["esnext", "dom"],
      "sourceMap": true,
      "jsx": "react",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "experimentalDecorators":true,
      "emitDecoratorMetadata": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "paths": {
        // 根据别名配置相关路径
        "@/*": ["./src/*"]
      }
    },

  }
